{
  "name": "MeoWoof",
  "description": "For dogs/cats lovers",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "dist/index.html",
    "default_icon": "src/assets/logo.png"
  },
  "icons": {
    "128": "src/assets/logo.png"
  }
}
